{
  "name": "@reaqtive/q",
  "version": "0.0.0",
  "description": "interfaces vs qlik with no layout components",
  "keywords": [
    "qlik",
    "react",
    "mashup"
  ],
  "private":false,
  "author": "paolo deregibus <paolo.deregibus@gmail.com>",
  "homepage": "",
